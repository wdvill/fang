.user_list li .name { height:15px; line-height:15px; overflow:hidden; padding-top:3px; width:50px; }
.topic_list { }
.topic_list li { line-height:21px; }
.ol_sz { font-size:14px; font-weight:bold; margin-right:5px; }
/*认证*/
